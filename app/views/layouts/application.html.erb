<!DOCTYPE html>
<html>
  <head>
    <title>Socialblog</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
         <header>
            <div>
               <p>My Logo</p>
               <p>Database</p>
            </div>
            <nav>
              <% if @current_user %>
              <h1><%= @current_user.name %></h1>
              <%= link_to "Sign Out", user_sign_out_path, method: :delete %>
              <% else %>
              <%= link_to "Sign_in",user_sign_in_path %>
              <%end%>
            </nav>
          </header>




    <%= yield %>
  </body>
</html>
